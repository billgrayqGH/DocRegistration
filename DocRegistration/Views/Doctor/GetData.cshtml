@model IEnumerable<DocRegistration.Models.Doctor>

@{
    ViewBag.Title = "BILL'S ONLINE DOCTOR REGISTRANTS";
}

<h2>BILL'S ONLINE DOCTOR REGISTRANTS</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoctorFirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoctorEmail)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Specialty)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoctorEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Specialty)
        </td>
       
    </tr>
}

</table>

<button type="button" class="btn btn-light">@Html.ActionLink("Go Back to Registration", "CreateDoctor", "Doctor")</button>
